{
  "name": "distense-frontend",
  "version": "0.3.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "analyze": "source-map-explorer build/main.*",
    "build": "node scripts/build.js",
    "dev": "DEV=true node scripts/start.js",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "node scripts/test.js --env=jsdom --coverage"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-control-regex": 0
    }
  },
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "babel-runtime": "^6.26.0",
    "bignumber.js": "^6.0.0",
    "classnames": "^2.2.6",
    "cross-fetch": "^2.2.2",
    "distense-contracts": "^1.2.2",
    "fbjs": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "meteor-random": "0.0.3",
    "npm": "^6.4.1",
    "object-assign": "4.1.1",
    "promise": "^8.0.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-autosuggest": "^9.4.2",
    "react-dom": "^16.5.2",
    "react-error-overlay": "^1.0.10",
    "react-helmet": "^5.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "run": "^1.4.0",
    "semantic-ui-react": "^0.87.3",
    "start": "^5.1.0",
    "styled-jsx": "^3.2.1",
    "truffle-contract": "^2.0.5",
    "web3": "^0.20.7",
    "web3-utils": "^1.0.0-beta.36"
  },
  "devDependencies": {
    "autoprefixer": "7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-airbnb": "^2.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "babel-preset-stage-3": "^6.24.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chai": "^4.1.2",
    "chai-jest-diff": "^1.0.2",
    "chalk": "1.1.3",
    "compression-webpack-plugin": "^1.1.12",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "0.11.2",
    "istanbul": "^0.4.5",
    "jest": "^22.4.4",
    "jsdom": "11.6.2",
    "mini-css-extract-plugin": "^0.8.0",
    "moment-timezone": "^0.5.21",
    "nyc": "^11.9.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "preload-webpack-plugin": "^3.0.0-beta.3",
    "prettier": "1.10.2",
    "react-dev-utils": "^6.0.0-next.3e165448",
    "react-test-renderer": "^16.5.2",
    "react-web3": "^1.2.0",
    "redux-devtools-extension": "^2.13.5",
    "sinon": "^4.5.0",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "^4.0.0",
    "webpack-auto-inject-version": "^1.1.0",
    "webpack-dev-server": "3.0.0"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "!src/**/(GetStarted|registerServiceWorker|SolidityEngineer|FAQ|FourOhFour|Jobs)*.js",
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.js?(x)"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/test/enzymeSetup.js"
  }
}
